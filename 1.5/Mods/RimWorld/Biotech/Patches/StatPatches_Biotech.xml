<?xml version="1.0" encoding="UTF-8"?>

<Patch>

    <!-- Fertility -->
    <Operation Class="ArchiteReinforcement.PatchOperationAddStatPart">
        <xpath>Defs/StatDef[defName="Fertility"]</xpath>
        <value>
            <li Class="ArchiteReinforcement.StatPart_ArchiteUpgrades">
                <architeDef>Turn_ArchiteUpgrade_Fertility</architeDef>
            </li>
        </value>
    </Operation>

    <!-- Learning Rate Factor (Child) -->
    <Operation Class="ArchiteReinforcement.PatchOperationAddStatPart">
        <xpath>Defs/StatDef[defName="LearningRateFactor"]</xpath>
        <value>
            <li Class="ArchiteReinforcement.StatPart_ArchiteUpgrades">
                <architeDef>Turn_ArchiteUpgrade_LearningRateFactor</architeDef>
            </li>
        </value>
    </Operation>

</Patch>